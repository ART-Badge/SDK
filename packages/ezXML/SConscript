from building import *

cwd     = GetCurrentDir()
# src     = Glob('*.c')
src     = []
CPPPATH = [cwd]

LOCAL_CCFLAGS = ''
import rtconfig
if rtconfig.CROSS_TOOL == 'keil':
    LOCAL_CCFLAGS += ' --gnu --diag_suppress=47,68,186,1293'

group = DefineGroup('ezXML', src, depend = ['RT_USING_DFS', 'PKG_USING_EZXML'], CPPPATH = CPPPATH,
    LOCAL_CCFLAGS = LOCAL_CCFLAGS)

Return('group')

